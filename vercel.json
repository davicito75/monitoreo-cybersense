{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/web/dist/assets/$1"
        },
        {
            "source": "/favicon.svg",
            "destination": "/web/dist/favicon.svg"
        },
        {
            "source": "/sw.js",
            "destination": "/web/dist/sw.js"
        },
        {
            "source": "/(.*)",
            "destination": "/web/dist/index.html"
        }
    ]
}